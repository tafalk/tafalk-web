{
  "name": "tafalk-web",
  "version": "0.2.1",
  "private": true,
  "dependencies": {
    "@aws-amplify/api": "^3.1.23",
    "@aws-amplify/auth": "^3.3.5",
    "@aws-amplify/core": "^3.4.6",
    "@aws-amplify/pubsub": "^3.0.24",
    "@aws-amplify/storage": "^3.2.13",
    "@date-io/date-fns": "^1.3.13",
    "@material-ui/core": "^4.11.0",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@material-ui/pickers": "^3.2.10",
    "@tafalk/material-color-generator": "^1.0.4",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.54",
    "@types/react-dom": "^16.9.8",
    "@types/react-loadable": "^5.5.3",
    "@types/react-router-dom": "^5.1.5",
    "@types/uuid": "^8.3.0",
    "axios": "^0.19.2",
    "date-fns": "^2.15.0",
    "debounce": "^1.2.0",
    "formik": "^2.1.5",
    "formik-material-ui": "^2.0.1",
    "formik-material-ui-pickers": "0.0.8",
    "graphql-tag": "^2.11.0",
    "i18next": "^19.7.0",
    "i18next-browser-languagedetector": "^4.3.1",
    "material-table": "1.68.0",
    "material-ui-cookie-consent": "^0.1.0",
    "mdi-material-ui": "^6.18.0",
    "notistack": "^0.9.17",
    "react": "^16.13.1",
    "react-cookie": "^4.0.3",
    "react-dom": "^16.13.1",
    "react-dropzone": "^11.0.3",
    "react-google-recaptcha-v3": "^1.5.2",
    "react-helmet": "^6.0.0",
    "react-i18next": "^11.7.0",
    "react-infinite-scroller": "^1.2.4",
    "react-loadable": "^5.5.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.3",
    "react-speech-recognition": "^3.2.0",
    "typescript": "^3.9.7",
    "uuid": "^8.3.0",
    "yup": "^0.28.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src --color",
    "format": "prettier --write src/**/*.{ts,tsx,scss,css,json}"
  },
  "eslintConfig": {
    "plugins": [
      "prettier",
      "@typescript-eslint"
    ],
    "extends": [
      "react-app",
      "prettier",
      "plugin:jsx-a11y/strict"
    ],
    "parserOptions": {
      "parser": "@typescript-eslint/parser",
      "project": "./tsconfig.json"
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {}
      ],
      "no-console": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/debounce": "^1.2.0",
    "@types/i18next-xhr-backend": "^1.4.2",
    "@types/react": "^16.9.46",
    "@types/react-helmet": "^6.1.0",
    "@types/react-infinite-scroller": "^1.2.1",
    "@types/react-speech-recognition": "^3.1.0",
    "@types/yup": "^0.26.37",
    "@types/zen-observable": "^0.8.0",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format && npm run lint"
    }
  },
  "lint-staged": {
    "*.+(js|ts|jsx|tsx)": [
      "eslint --fix",
      "git add"
    ],
    "*.+(json|css|md)": [
      "prettier --write",
      "git add"
    ]
  }
}
