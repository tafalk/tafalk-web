{
  "name": "tafalk-web",
  "version": "1.2.4",
  "private": true,
  "dependencies": {
    "@aws-amplify/api": "^3.2.29",
    "@aws-amplify/auth": "^3.4.29",
    "@aws-amplify/core": "^3.8.21",
    "@aws-amplify/pubsub": "^3.2.27",
    "@aws-amplify/storage": "^3.3.29",
    "@date-io/date-fns": "^1.3.13",
    "@material-ui/core": "^4.11.3",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@material-ui/pickers": "^3.3.10",
    "@tafalk/material-color-generator": "^1.0.9",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^13.1.1",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.37",
    "@types/react-dom": "^17.0.3",
    "@types/react-loadable": "^5.5.5",
    "@types/react-router-dom": "^5.1.7",
    "@types/uuid": "^8.3.0",
    "axios": "^0.21.1",
    "date-fns": "^2.19.0",
    "debounce": "^1.2.1",
    "formik": "^2.2.6",
    "formik-material-ui": "^3.0.1",
    "formik-material-ui-pickers": "0.0.12",
    "graphql-tag": "^2.11.0",
    "i18next": "^20.2.0",
    "i18next-browser-languagedetector": "^6.1.0",
    "material-table": "^1.69.2",
    "material-ui-cookie-consent": "^0.1.0",
    "mdi-material-ui": "^6.22.0",
    "notistack": "^1.0.5",
    "react": "^17.0.2",
    "react-cookie": "^4.0.3",
    "react-dom": "^17.0.2",
    "react-dropzone": "^11.3.2",
    "react-google-recaptcha-v3": "^1.8.1",
    "react-helmet": "^6.0.0",
    "react-i18next": "^11.8.12",
    "react-infinite-scroller": "^1.2.4",
    "react-loadable": "^5.5.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "react-speech-recognition": "^3.7.0",
    "typescript": "^4.2.3",
    "uuid": "^8.3.1",
    "yup": "^0.32.9"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src --color",
    "format": "prettier --write src/**/*.{ts,tsx,scss,css,json}"
  },
  "eslintConfig": {
    "plugins": [
      "prettier",
      "@typescript-eslint"
    ],
    "extends": [
      "react-app",
      "prettier",
      "plugin:jsx-a11y/strict"
    ],
    "parserOptions": {
      "parser": "@typescript-eslint/parser",
      "project": "./tsconfig.json"
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {}
      ],
      "no-console": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/debounce": "^1.2.0",
    "@types/i18next-xhr-backend": "^1.4.2",
    "@types/react": "^17.0.3",
    "@types/react-helmet": "^6.1.0",
    "@types/react-infinite-scroller": "^1.2.1",
    "@types/react-speech-recognition": "^3.6.0",
    "@types/yup": "^0.29.11",
    "@types/zen-observable": "^0.8.2",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format && npm run lint"
    }
  },
  "lint-staged": {
    "*.+(js|ts|jsx|tsx)": [
      "eslint --fix",
      "git add"
    ],
    "*.+(json|css|md)": [
      "prettier --write",
      "git add"
    ]
  }
}
